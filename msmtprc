defaults
tls on
tls_starttls on
auth on
logfile ~/.mail/msmtp.log
syslog off

account thalmeier
host mail.michisoft.net
port 25
tls_certcheck off
from michael@thalmeier.at
user michael@thalmeier.at

account michisoft
host mail.michisoft.net
port 25
tls_certcheck off
from office@michisoft.net
user michi@michisoft.net

account fftarsdorf
host mail.michisoft.net
port 25
tls_certcheck off
from thalmeierm@ff-tarsdorf.at
user thalmeierm@ff-tarsdorf.at

account default : thalmeier
